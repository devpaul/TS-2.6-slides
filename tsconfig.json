{
	"version": "2.7",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"jsx": "react",
		"module": "umd",
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"importHelpers": true,
		"downlevelIteration": true,
		"outDir": "_build/",
		"removeComments": false,
		"sourceMap": true,
		"strict": true,
		"strictFunctionTypes": true,
		"target": "es6"
	},
	"include": [
		"./samples/**/*.js",
		"./samples/**/*.ts"
	]
}
